{
    "name": "cwalinapj/ai-wp-dynamic-cache",
    "description": "Signed edge agent cache plugin with Cloudflare Workers control plane.",
    "type": "wordpress-plugin",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "yoast/phpunit-polyfills": "^2.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "AiWpCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AiWpCache\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --testdox",
        "analyse": "phpstan analyse src tests --level=8"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
